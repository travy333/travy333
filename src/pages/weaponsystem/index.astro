---
import BaseLayout from "../../layouts/BaseLayout.astro";
import Header from "../../components/Header.astro";
import Footer from "../../components/Footer.astro";
import ProjectPage from "../../components/ProjectPage.astro";


const projectData = {
  title: "Weapon System",
  description: "Classi per definire armi e munizioni, meccaniche di sparo, ricarica e  tanto altro. Compatibile con Unreal Engine 5.",
  imageUrl: "/assets/travyplay-screenshot.jpg", 
  tags: ["UE5","C++"]
};
---


<BaseLayout title={projectData.title}>
  <Fragment slot="header">
  
    <Header title={projectData.title} />
  </Fragment>

  <ProjectPage {...projectData} />

  <Fragment slot="footer"><Footer /></Fragment>
</BaseLayout>


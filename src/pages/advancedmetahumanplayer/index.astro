---
import BaseLayout from "../../layouts/BaseLayout.astro";
import Header from "../../components/Header.astro";
import Footer from "../../components/Footer.astro";
import ProjectPage from "../../components/ProjectPage.astro";


const projectData = {
  title: "Advanced Metahuman Player",
  description: "Standard player riutilizzabile che utilizza Metahumans super fedeli, ottimizzati per il gaming.",
  imageUrl: "/assets/travyplay-screenshot.jpg", 
  tags: ["UE5","C++"]
};
---


<BaseLayout title={projectData.title}>
  <Fragment slot="header">
  
    <Header title={projectData.title} />
  </Fragment>

  <ProjectPage {...projectData} />

  <Fragment slot="footer"><Footer /></Fragment>
</BaseLayout>

